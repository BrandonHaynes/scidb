--setup
create array source <a: int32> [i=0:9999,10000,250,j=0:9999,10000,250]
create array result <a: int32> [i=0:9999,10000,500,j=0:9999,10000,500]
--start-igdata
--start-timer store
store(filter(build(source, i*i+j*j), (i % 10000 = 0) or (j % 10000 = 0)), source)
--stop-timer store
--stop-igdata
--test
--start-igdata
store(repart(source, result), result)
--start-timer repart_0
store(repart(source, result), result)
--stop-timer repart_0
--start-timer repart_1
store(repart(source, result), result)
--stop-timer repart_1
--start-timer repart_2
store(repart(source, result), result)
--stop-timer repart_2
--stop-igdata
--cleanup
remove(source)
remove(result)