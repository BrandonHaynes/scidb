--setup
--start-query-logging

# NOTE: NNcNN notation means size Rows,Cols ; chunkSize Rows,Cols

# identity: 11c11, 22c11, 22c22
create array ident1<a: double >   [r=0:0,32,0, c=0:0,32,0]
create array ident2<a: double >   [r=0:1,32,0, c=0:1,32,0]
create array ident4<a: double >   [r=0:3,32,0, c=0:3,32,0]
create array ident8<a: double >   [r=0:7,32,0, c=0:7,32,0]
create array ident16<a: double >   [r=0:15,32,0, c=0:15,32,0]
create array ident32<a: double >   [r=0:31,32,0, c=0:31,32,0]
create array ident64<a: double >   [r=0:63,32,0, c=0:63,32,0]
create array ident128<a: double >   [r=0:127,32,0, c=0:127,32,0]
create array ident256<a: double >   [r=0:255,32,0, c=0:255,32,0]
create array ident512<a: double >   [r=0:511,32,0, c=0:511,32,0]
create array ident1024<a: double >   [r=0:1023,32,0, c=0:1023,32,0]
create array ident2048<a: double >   [r=0:2047,32,0, c=0:2047,32,0]
create array ident4096<a: double >   [r=0:4095,32,0, c=0:4095,32,0]
create array ident8192<a: double >   [r=0:8191,32,0, c=0:8191,32,0]
#create array ident16384<a: double >   [r=0:16383,32,0, c=0:16383,32,0]
#create array ident32768<a: double >   [r=0:32767,32,0, c=0:32767,32,0]
#create array ident65536<a: double >   [r=0:65535,32,0, c=0:65535,32,0]

load_library('dense_linear_algebra')

--test

--shell --command "echo 1 >&2; /usr/bin/time -f%E iquery -naq 'gemm(build(ident1,1), build(ident1,1), build(ident1,0))'"
--shell --command "echo 2 >&2; /usr/bin/time -f%E iquery -naq 'gemm(build(ident2,1), build(ident2,1), build(ident2,0))'"
