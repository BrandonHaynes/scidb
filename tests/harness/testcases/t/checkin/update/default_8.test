--setup
--start-query-logging
create array A <a:int32 default 5> [x=0:2,3,0,y=2:5,2,1]

--test
--aql select * into A from filter(build(A,x+3),true)
--aql select * into A from filter(build(A,x+y+3),x<>1)
--aql select a+0 from A
--aql update A set a = a + 1
--aql update A set a = a - 1
--aql update A set a = a * 1
--aql update A set a = a / 1
--aql update A set a = a * 10
--aql update A set a = a / 10
--aql update A set a = a / 2
--aql select * into A from filter(build(A,x+3),true)

--cleanup
remove(A)
--stop-query-logging
