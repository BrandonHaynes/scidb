--setup
--start-query-logging
create array A <a:int64> [x=0:9,2,0]
create array B <x:int64> [a=0:14,3,0]
create array D <a:int64> [x=0:9,3,0]
store(build(A,x+3),A)
store(B,C)
store(D,E)
store(filter(build(D,x+1),x<>1),D)

--test
store(redimension(A,B),B)
store(redimension(D,C),C)
unpack(B,row)
unpack(C,row)
store(redimension(B,E),E)

aggregate(B,max(x))
aggregate(B,min(x))
aggregate(B,avg(x))
rename(B,B1)
rename(B1,B)
subarray(B,10,11)
between(B,11,20)
filter(B,x>1)
project(B,x)
join(B,B)
cross_join(B,B)
apply(B,z,x-a)
aggregate(B,count(*))
aggregate(B,sum(x))
sort(B,x)
sort(B,x desc)

aggregate(C,max(x))
aggregate(C,min(x))
aggregate(C,avg(x))
rename(C,C1)
rename(C1,C)
subarray(C,10,11)
between(C,10,20)
filter(C,x>1)
project(C,x)
join(C,C)
cross_join(C,C)
apply(C,z,x-a)
aggregate(C,count(*))
aggregate(C,sum(x))
sort(C,x)
sort(C,x desc)

aggregate(E,max(a))
aggregate(E,min(a))
aggregate(E,avg(a))
rename(E,E1)
rename(E1,E)
subarray(E,5,7)
between(E,4,6)
filter(E,x>1)
project(E,a)
join(E,E)
cross_join(E,E)
apply(E,z,x-a)
aggregate(E,count(*))
aggregate(E,sum(a))
sort(E,a)
sort(E,a desc)

--cleanup
remove(A)
remove(B)
remove(C)
remove(D)
remove(E)
--stop-query-logging
