--setup
--start-query-logging
create array A <a:int32> [x=-3:7,3,2,y=0:2,3,2]
store(filter(build(A,x*10+y),x<>2 and x<>5),A)

--test
regrid(A,1,1,sum(a))
regrid(A,1,1,max(a))
regrid(A,1,1,min(a))
regrid(A,1,1,avg(a))
regrid(A,1,1,count(a))
regrid(A,1,1,count(*))
regrid(A,1,1,stdev(a))
regrid(A,1,1,var(a))


store(build(A,x-2),A)
regrid(A,18,18,sum(a),max(a))
regrid(A,3,2,max(a),var(a),stdev(a),count(a),sum(a),min(a))
regrid(A,4,3,min(a),count(*))
regrid(A,1,4,avg(a),count(a))
regrid(A,2,5,count(a),count(*))
regrid(A,3,6,count(*),sum(a))
regrid(A,5,7,stdev(a),avg(a))
regrid(A,9,1,var(a),stdev(a))


--cleanup
remove(A)
--stop-query-logging
