--setup
--start-query-logging
create array A <a:int32 compression 'rle', b:double compression 'no compression'> [x=-1:2,3,2,y=0:2,2,0]
create array T <a:int32 default 9> [x=-1:2,3,2,y=0:2,2,0]
create array T2 <a:double default 5.5> [x=-1:2,3,2,y=0:2,2,0]
store(join(filter(build(T,x+y+5),x<>2),filter(build(T2,x+y+10.2),x<>2)),A)

