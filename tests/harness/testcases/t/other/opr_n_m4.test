--setup
--start-query-logging
create array A <a:int32 null> [x=-3:7,3,2,y=0:2,3,2]
store(filter(build(A,x*10+y),x<>2 and x<>5),A)

--test
--aql update A set a=null where x=1 or x=6
apply(A,z,a+10)
between(A,0,0,2,3)
cross_join(A,A)
cross_join(A as a1,A as a2,a1.x,a2.x)
concat(A,A)
filter(A,x%2=1)
filter(A,x%2<>1)
join(A,A)
merge(A,A@1)
merge(A@1,A)
project(apply(A,z,a+10),z)
reverse(A)
#reshape(A,<a:int32 null> [x=-3:7,3,2,y=0:2,3,2])
repart(A,<a:int32 null> [x=-3:7,2,2,y=0:2,2,2])
--igdata "bernoulli(A,0.5,1)"
--igdata "bernoulli(A,0.5,1)"
lookup(join(build(<x:int64>[j=0:2,3,0],j),build(<y:int64>[i=0:2,3,0],i)),A)
store(build(A,iif(x=1,null,x*10+y)),A)
slice(A,x,1)
#sort(A,a desc)
#sort(A,a)
store(build(A,iif(x=3,null,x+10)),A)
thin(A,-2,3,1,3)
transpose(A)
#unpack(A,z)
xgrid(A,1,2)


--cleanup
remove(A)
--stop-query-logging
