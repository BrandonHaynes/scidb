--igdata "load(Dsp_mult, 'Dsp_mult.bin', -1, 'opaque')"
# Wrong file. Throw an error
--error "load(Dsp, 'expo.bin', -2, 'opaque')"

--cleanup
remove(Ds)
remove(Dp)
remove(Dsp)
remove(Dsp2)
remove(Dsp_d)
remove(Dsp3)

remove(Dsp_mult_d)
remove(Dsp_mult)

remove(expo_1d)
remove(expo)
remove(expo2)

remove(Dsp_id)
remove(expo_1d_a)

--stop-ignore-warnings
--stop-query-logging
