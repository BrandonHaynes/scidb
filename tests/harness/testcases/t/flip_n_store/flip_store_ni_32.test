--setup
--start-query-logging
create array A <a:double,b:double>[x=0:2,3,0]
create array T <a:double>[x=0:2,3,0]
create array B <b:double>[a(double)=3,3,0]
create array C <a:double>[b(double)=3,3,0]
store(join(build(T,x+1+0.05),build(T,x+5+0.08)),A)

--test

#workaround for p4 bug 564
--set-format csv+

store(redimension(A,B),B)
store(redimension(B,C),C)
unpack(B,row)
unpack(C,row)

rename(B,B1)
aggregate(B1,max(b))
aggregate(B1,min(b))
subarray(B1,1,2)
subarray(B1,1,3)
subarray(B1,double(1),double(2))
subarray(B1,double(1),2.05)
between(B1,1,2)
between(B1,1,3)
between(B1,double(1),double(2))
between(B1,double(1),2.05)
filter(B1,a>2)
filter(B1,a<2)
filter(B1,a=2.05)
filter(B1,a<>2.05)
filter(B1,b>5.08)
filter(B1,b<6.09)
filter(B1,b=7.08)
filter(B1,b<>7.08)
project(B1,b)
join(B1,B1)
cross_join(B1,B1)
apply(B1,z,a+0.0001)
apply(B1,z,b+0.0001)
aggregate(B1,count(*))
aggregate(B1,sum(b))
sort(B1,b)
sort(B1,b desc)
rename(B1,B)

rename(C,C1)
aggregate(C1,max(a))
aggregate(C1,min(a))
subarray(C1,5,6)
subarray(C1,5,7)
subarray(C1,double(5),double(6))
subarray(C1,6.08,7.08)
between(C1,5,6)
between(C1,5,7)
between(C1,double(5),double(6))
between(C1,6.08,7.08)
filter(C1,a>1.05)
filter(C1,a<2)
filter(C1,a=1.05)
filter(C1,a<>1.05)
filter(C1,b>6)
filter(C1,b<6)
filter(C1,b=7.08)
filter(C1,b<>7.08)
project(C1,a)
join(C1,C1)
cross_join(C1,C1)
apply(C1,z,a+0.00001)
apply(C1,z,b+0.00001)
aggregate(C1,count(a))
aggregate(C1,sum(a))
sort(C1,a)
sort(C1,a desc)
rename(C1,C)

--reset-format

--cleanup
remove(A)
remove(B)
remove(C)
remove(T)
--stop-query-logging
