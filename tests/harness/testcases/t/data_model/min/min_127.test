
--setup
--start-query-logging
--start-igdata
--aql create  array T < x: int64 > [ i=1:1050,100,0, j=1:1050,100,0 ]
--aql create  array E < x: int64 null > [ j=1:1050,100,0 ]
--aql create  array R < x: int64 null > [ j=1:1050,100,0 ]
store(build(T,i+j),T)
store(build(E,j+1),E)
store(repart(aggregate(T,min(x),j),R),R)
--stop-igdata

--test
--aql SELECT count(*) FROM R , E WHERE abs((R.x-E.x)/iif(E.x=0,1,E.x)) > 1e-07

--cleanup
remove(T)
remove(E)
remove(R)
--stop-query-logging