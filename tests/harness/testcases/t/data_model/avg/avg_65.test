
--setup
--start-query-logging
--start-igdata
--aql create  array T < x: double > [ i=1:1001,101,0, j=1:999,99,0 ]
--aql create  array E < x: double null > [ j=1:999,101,0 ]
--aql create  array R < x: double null > [ j=1:999,101,0 ]
store(build(T,i+j),T)
store(build(E,j+(1001+1.)/2.),E)
store(repart(aggregate(T,avg(x),j),R),R)
--stop-igdata

--test
--aql SELECT count(*) FROM R , E WHERE abs((R.x-E.x)/iif(E.x=0,1,E.x)) > 1e-07

--cleanup
remove(T)
remove(E)
remove(R)
--stop-query-logging