
--setup
--start-query-logging
--start-igdata
--aql create array T < x: int64 > [ i=1:3,2,0, j=1:4,3,0 ]
--aql create array E < x: double null > [ i=1:3,2,0 ]
--aql create array R < x: double null > [ i=1:3,2,0 ]
store(build(T,i+j),T)
store(build(E,i+(4+1.)/2.),E)
store(repart(aggregate(T,avg(x),i),R),R)
--stop-igdata

--test
--aql SELECT count(*) FROM R , E WHERE abs((R.x-E.x)/iif(E.x=0,1,E.x)) > 1e-07

--cleanup
remove(T)
remove(E)
remove(R)
--stop-query-logging