
--setup
--start-query-logging
--start-igdata
--aql create   array T < x: int64 > [ i=1:3,2,0, j=1:4,3,0 ]
--aql create  array E < x: int64 null > [ i=0:0,1,0 ]
--aql create  array R < x: int64 null > [ i=0:0,1,0 ]
store(filter(build(T,i+j),j-2<i and i<j+2),T)
store(build(E,iif(3>4, 3*4*(4+1)-1, 3*3*(3+1)-1)),E)
store(repart(aggregate(T, sum(x)),R),R)
--stop-igdata

--test
--aql SELECT count(*) FROM R , E WHERE abs((R.x-E.x)/iif(E.x=0,1,E.x)) > 1e-07

--cleanup
remove(T)
remove(E)
remove(R)
--stop-query-logging