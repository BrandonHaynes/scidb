
--setup
--start-query-logging
--start-igdata
--aql create  array T < x: double > [ i=1:13,13,0, j=1:21,21,0 ]
--aql create  array E < x: double null > [ j=1:21,13,0 ]
--aql create  array R < x: double null > [ j=1:21,13,0 ]
store(build(T,i-j),T)
store(build(E,13*(13+1)/2 - j*13),E)
store(repart(aggregate(T,sum(x),j),R),R)
--stop-igdata

--test
--aql SELECT count(*) FROM R , E WHERE abs((R.x-E.x)/iif(E.x=0,1,E.x)) > 1e-07

--cleanup
remove(T)
remove(E)
remove(R)
--stop-query-logging